{
  "name": "examples",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "options": {
        "main": "packages/sdk/examples/src/index.ts",
        "outputPath": "packages/sdk/examples/dist/types",
        "tsConfig": "packages/sdk/examples/tsconfig.json"
      },
      "outputs": ["{options.outputPath}"]
    },
    "bundle": {
      "executor": "@nx/vite:build",
      "options": {
        "outputPath": "packages/sdk/examples/out/demo"
      },
      "outputs": ["{options.outputPath}"]
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "format": "unix",
        "lintFilePatterns": ["packages/sdk/examples/**/*.{ts,tsx,js,jsx}"],
        "quiet": true
      }
    },
    "prebuild": {
      "executor": "nx:run-script",
      "options": {
        "script": "prebuild"
      },
      "outputs": ["{projectRoot}/src/proto/gen"]
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "options": {
        "buildTarget": "examples:bundle"
      }
    },
    "storybook": {
      "configurations": {
        "ci": {
          "quiet": true
        }
      },
      "executor": "@nx/storybook:storybook",
      "options": {
        "configDir": "packages/sdk/examples/.storybook"
      }
    },
    "storybook-build": {
      "configurations": {
        "ci": {
          "quiet": true
        }
      },
      "executor": "@nx/storybook:build",
      "options": {
        "configDir": "packages/sdk/examples/.storybook",
        "outputDir": "packages/sdk/examples/out"
      },
      "outputs": ["{options.outputDir}"]
    }
  }
}
