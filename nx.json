{
  "extends": "nx/presets/npm.json",
  "affected": {
    "defaultBase": "main"
  },
  "namedInputs": {
    "default": ["{projectRoot}/**/*", "sharedGlobals"],
    "sharedGlobals": ["{workspaceRoot}/tsconfig.json", "{workspaceRoot}/.eslintrc*.js"],
    "production": [
      "default",
      "!{projectRoot}/**/*.spec.ts",
      "!{projectRoot}/**/*.test.ts",
      "!{projectRoot}/**/*.md",
      "!{projectRoot}/eslintConfig.json",
      "!{workspaceRoot}/.eslintrc*.js"
    ]
  },
  "targetDefaults": {
    "build": {
      "inputs": ["production", "^production"],
      "dependsOn": ["^build", "prebuild", "compile"],
      "cache": true
    },
    "bundle": {
      "inputs": [
        "production",
        "^production",
        {
          "env": "CONFIG_DYNAMIC"
        },
        {
          "env": "DX_ENVIRONMENT"
        },
        {
          "env": "DX_IPDATA_API_KEY"
        },
        {
          "env": "DX_SENTRY_DESTINATION"
        },
        {
          "env": "DX_TELEMETRY_API_KEY"
        },
        {
          "env": "DX_VAULT"
        },
        {
          "env": "NODE_ENV"
        },
        {
          "env": "REMOTE_SOURCE"
        },
        {
          "env": "SENTRY_RELEASE_AUTH_TOKEN"
        }
      ],
      "dependsOn": ["^build", "prebuild"],
      "cache": true
    },
    "chromatic": {
      "inputs": ["production", "^production"],
      "dependsOn": ["storybook-build"]
    },
    "compile": {
      "inputs": ["production", "^production"],
      "dependsOn": ["^compile", "prebuild"],
      "cache": true
    },
    "e2e": {
      "inputs": [
        "default",
        "^production",
        {
          "env": "CI"
        },
        {
          "env": "LOG_LEVEL"
        },
        {
          "env": "LOG_PROCESSOR"
        }
      ],
      "dependsOn": ["prebuild", "^compile"],
      "cache": true
    },
    "lint": {
      "inputs": ["default"],
      "cache": true
    },
    "prebuild": {
      "inputs": ["production", "^production"],
      "dependsOn": ["^compile"],
      "cache": true
    },
    "prerelease": {
      "inputs": ["default", "^production"],
      "dependsOn": ["^build"],
      "cache": true
    },
    "preview": {
      "inputs": ["production", "^production"]
    },
    "serve": {
      "inputs": ["production", "^production"],
      "dependsOn": ["^compile", "prebuild"]
    },
    "serve-with-vault": {
      "inputs": ["production", "^production"],
      "dependsOn": ["^compile", "prebuild"]
    },
    "storybook": {
      "inputs": ["production", "^production"],
      "dependsOn": ["^compile", "prebuild"]
    },
    "storybook-build": {
      "inputs": ["production", "^production"],
      "dependsOn": ["^compile", "prebuild"],
      "cache": true
    },
    "test": {
      "inputs": [
        "default",
        "^production",
        {
          "env": "CI"
        },
        {
          "env": "LOG_LEVEL"
        },
        {
          "env": "LOG_PROCESSOR"
        }
      ],
      "dependsOn": ["prebuild", "^compile"],
      "cache": true
    },
    "compile-lib": {
      "cache": true
    },
    "compile-node": {
      "cache": true
    }
  },
  "nxCloudAccessToken": "ZGVlNTQ0ODctNGI4ZS00MTc0LTljZDQtMzFlMzJkOTViMmQyfHJlYWQtd3JpdGU=",
  "parallel": 6
}
